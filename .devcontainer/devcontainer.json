{
  "name": "Pruva Reproduction Environment",
  "image": "ghcr.io/n3mes1s/pruva-sandbox:latest",
  "postStartCommand": ".devcontainer/run-reproduction.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
      }
    },
    "codespaces": {
      "openFiles": []
    }
  },
  "remoteUser": "vscode",
  "containerEnv": {
    "PRUVA_SANDBOX": "true",
    "PRUVA_API_URL": "https://pruva-api-production.up.railway.app/v1"
  }
}
